exportPattern("^[[:alpha:]]+")

S3method(print,timetable)
S3method(summary,timetable)
S3method("[",timetable)
S3method("[<-" ,timetable)
S3method(rbind,timetable)
S3method(cbind,timetable)
S3method(dim,timetable)
S3method(nrow,timetable)
S3method(ncol,timetable)
S3method(names,timetable)
S3method("names<-",timetable)
S3method(time,timetable)
